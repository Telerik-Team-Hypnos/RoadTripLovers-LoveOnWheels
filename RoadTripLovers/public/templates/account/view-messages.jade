.modal.fade#view-messages-modal(role="dialog" aria-hidden="true")
	.modal-dialog
		.modal-content
			.modal-header
				span Messages
				button.close(data-dismiss="modal") x					
			.modal-body
				ul
					li( ng-repeat="messageo in logedUserMessages") {{messageo.title}}{{messageo.body}}{{messageo.postTime}}{{messageo.sender.username}}
			.modal-footer
				button.btn.btn-default(data-dismiss="modal") Close				